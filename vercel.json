{
  "headers": [
    {
      "source": "/your-file.pdf",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/your-file.pdf",
      "dest": "/public/your-file.pdf"
    }
  ]
}
